<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.maps3"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/radar"
        android:label="FoodRadar"
        android:theme="@style/AppTheme" >
        <uses-library android:name="com.google.android.maps" />
         <activity
            android:name="com.example.maps3.Home"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.HOME" />
          <category android:name="android.intent.category.DEAFULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.maps3.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.example.maps3.Demo"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAPS" />
               <category android:name="android.intent.category.DEFAULT" />
               
            </intent-filter>
        </activity>
         <activity
            android:name="com.example.maps3.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MENU" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.example.maps3.Options"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.OPTIONS" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
 	<activity
            android:name="com.example.maps3.Sudexo"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.SUDEXO" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.maps3.Dishes"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.DISHES" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.maps3.DishResult"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.DISHRESULT" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.maps3.Review"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.REVIEW" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
         <activity
            android:name="com.example.maps3.ReviewResult"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.REVIEWRESULT" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
       <activity
            android:name="com.example.maps3.PlaceSelect"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.PLACESELECT" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.maps3.PlaceResult"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.PLACERESULT" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.maps3.Maps2"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAPS2" />
          <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    
    </application>

</manifest>